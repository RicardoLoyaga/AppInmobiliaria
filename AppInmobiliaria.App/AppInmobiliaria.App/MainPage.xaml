        <Entry x:Name="txtContrasena" />
        <Button x:Name="btnIniciarSesion" Text="Iniciar Sesión" BackgroundColor="#7d332c" TextColor="White" CornerRadius="15" />
    </StackLayout>

</ContentPage>
